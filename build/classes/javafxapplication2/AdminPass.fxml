<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="javafxapplication2.AdminPassController">
  <children>
    <AnchorPane layoutX="0.0" layoutY="0.0" prefHeight="86.0" prefWidth="600.0" style="&#10;&#10;-fx-background-color:gold; &#10;">
      <children>
        <Label layoutX="167.0" layoutY="38.0" text="Admin PassWord Change">
          <font>
            <Font name="System Bold" size="24.0" />
          </font>
        </Label>
        <Button fx:id="back" layoutX="14.0" layoutY="33.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color:red;&#10;&#10;-fx-background-radius:35;" text="Back" />
      </children>
    </AnchorPane>
    <VBox layoutX="167.0" layoutY="112.0" prefHeight="263.0" prefWidth="285.0" spacing="15.0">
      <children>
        <Label text="ID">
          <font>
            <Font name="System Bold" size="15.0" fx:id="x1" />
          </font>
        </Label>
        <TextField fx:id="id" prefWidth="200.0" />
        <Label font="$x1" text="Old Password" VBox.vgrow="ALWAYS" />
        <PasswordField fx:id="previousPassword" prefWidth="200.0" />
        <Label font="$x1" text="New Password" />
        <PasswordField fx:id="newPassword" prefWidth="200.0" />
        <Label font="$x1" text="Confirm New Password" />
        <PasswordField fx:id="confirmNewPassword" prefWidth="200.0" />
      </children>
    </VBox>
    <Button fx:id="confirm" layoutX="270.0" layoutY="403.0" mnemonicParsing="false" onAction="#confirmButton" style="-fx-background-color:lightgreen;&#10;&#10;-fx-background-radius:35;" text="Confirm" />
  </children>
</AnchorPane>
